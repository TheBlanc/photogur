<h1>Sign In</h1>
<%= link_to("Back to Pictures", root_url) %>

<%= form_for :session, url: session_path do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit "Sign In" %>
  </p>
<% end %>
